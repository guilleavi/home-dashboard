    "react-dom": "^18.2.0",
    "ui": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "18.7.16",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.23.1",
    "eslint-config-next": "^12.3.0",
    "prettier": "^2.7.1",
    "prettier-config-custom": "workspace:*",
    "prisma": "^4.6.1",
    "sass": "^1.54.9",
